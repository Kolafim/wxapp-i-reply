@import '../template/signup/signup.wxss';
.content-cell {
  /* height:40%; */
  border-bottom:1px solid #f3f3f3;
  position:relative;
  margin-bottom:50rpx;
  padding:20rpx 30rpx 50rpx;
  overflow: hidden;
}
.content-area {
  min-height:40vh;
  max-height:70vh;
}
.content-cell .corsur-text {
  position:absolute;
  right:20rpx;
  bottom:10rpx;
  color:#ccc;
  font-size: 28rpx;
}
.btn-item {
  background: #fff;
  font-size:30rpx;
  border-top:1px solid #f3f3f3;
  border-bottom:1px solid #f3f3f3;
  line-height: 80rpx;
  margin:20rpx 0;
}
.btn-item:active {
  background: #fbfbfb;
}
